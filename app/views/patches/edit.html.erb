<%= form_for(@patch, as: :patch, url: user_patch_path(@user, @patch)) do |f| %>
  <%= f.file_field :file %><br /><br />
  Patch Name: <%= f.text_field :name, :value => @patch.name %><br /><br />
  If from a game - which one?: <%= f.text_field :game %><br /><br />
  Original?: <%= f.check_box :original %><br /><br />
  <%= f.submit 'Edit Patch'%>
<% end %>
